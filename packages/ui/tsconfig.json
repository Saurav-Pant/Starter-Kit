{
  "extends": "tsconfig/react-library.json",
  "compilerOptions": {
    "baseUrl": ".", 
    "paths": {
      "@ui/*": ["./src/*"],
      "ui2": ["./src/components/ui/index.ts"]
    },
    "moduleResolution": "node", 
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src", "index.ts"],
  "exclude": ["node_modules"]
}
